<h2>User List</h2>

<table border="1">
  <tr>
    <th>Id</th>
    <th>Email</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Admin</th>
    <th>Created At</th>
    <th>Updated At</th>
  </tr>
  <%= paginate @users %>
  <% @users.each do |user| %>
    <tr>
      <td><%= link_to 'Masquerade', new_user_masquerade_path(user) %></td>
      <td><%= user.id %></td>
      <td><%= link_to user.email, admin_user_path(user) %></td>
      <td><%= user.firstname%></td>
      <td><%= user.lastname%></td>
      <td><%= user.admin%></td>
      <td><%= user.created_at%></td>
      <td><%= user.updated_at%></td>
    </tr>
  <% end %>
</table>